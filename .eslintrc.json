{
  "env": { "browser": true, "node": true, "es6": true },
  "extends": ["eslint:recommended", "next/core-web-vitals"],
 
  "rules": {
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "extends",
        "tailwind"
      ]
    }],
    "block-no-empty": null,
    "unit-allowed-list": ["em", "rem", "s"]
  }
}
